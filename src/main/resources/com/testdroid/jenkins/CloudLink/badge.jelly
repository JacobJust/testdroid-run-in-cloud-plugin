<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core">
    <j:if test="${it.hasLink()}">
        <a href="${it.getUrlName()}">
            <img width="16" height="16"
                 title="${it.getDisplayName()}"
                 src="${it.getIconFileName()}"/>
        </a>
    </j:if>
</j:jelly>